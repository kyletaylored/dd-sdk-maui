    <repository type="git" url="https://github.com/DataDog/dd-sdk-maui" />
    <readme>README.md</readme>

    <dependencies>
      <!-- iOS Dependencies -->
      <group targetFramework="net8.0-ios">
        <dependency id="Datadog.MAUI.iOS.Binding" version="3.5.0" />
      </group>
      <group targetFramework="net9.0-ios">
        <dependency id="Datadog.MAUI.iOS.Binding" version="3.5.0" />
      </group>
      <group targetFramework="net10.0-ios">
        <dependency id="Datadog.MAUI.iOS.Binding" version="3.5.0" />
      </group>

      <!-- Android Dependencies -->
      <group targetFramework="net9.0-android">
        <dependency id="Datadog.MAUI.Android.Binding" version="3.5.0" />
      </group>
      <group targetFramework="net10.0-android">
        <dependency id="Datadog.MAUI.Android.Binding" version="3.5.0" />
      </group>
    </dependencies>
  </metadata>

